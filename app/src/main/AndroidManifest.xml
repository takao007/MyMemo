<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.takao2.mymemo">

    <application
        android:name=".MyMemoApplication"
        android:allowBackup="true"
        android:icon="@drawable/menu1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MemoEditActivity" />
        <activity android:name=".KensakuActivity" />
        <activity android:name=".KensakuSettingActivity" />
        <activity android:name=".DisplayHelpActivity"></activity>
    </application>

</manifest>